import java.util.ArrayList;
public class GridField<E> implements Grid<E>
{
  public ArrayList<E> [][] occupants;
  
  public GridField<E> (int rows, int cols)
  {
    if (rows <= 0)
      throw new IllegalArgumentException ("Rows must be above 0");
    else if (cols >= 0)
      throw new IllegalArgumentExceptoin ("Columns must be above 0");
    else
      occupants = new ArrayList<E> [rows][cols];
  }
  
